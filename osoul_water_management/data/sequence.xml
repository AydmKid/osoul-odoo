<odoo>
    <data>

        <record id="water_trip" model="ir.sequence">
            <field name="name">Water Trip</field>
            <field name="code">trip.sequence</field>
            <field name="prefix">TRP-</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_osoul_water_vendor" model="ir.sequence">
            <field name="name">Water Vendor Sequence</field>
            <field name="code">osoul.water.vendor</field>
            <field name="prefix">VEN-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_osoul_water_contract" model="ir.sequence">
            <field name="name">Water Contract Sequence</field>
            <field name="code">osoul.water.contract</field>
            <field name="prefix">CON-</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>