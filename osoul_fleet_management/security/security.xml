<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="module_category_fleet" model="ir.module.category">
        <field name="name">Fleet Management</field>
        <field name="description">hr</field>
        <field name="sequence" eval="10" />
    </record>

    <record id="group_fleet_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="comment">Managers Group</field>
        <field name="category_id" ref="module_category_fleet"/>
    </record>

    <record id="group_fleet_supervisor" model="res.groups">
        <field name="name">Supervisor</field>
        <field name="comment">Supervisors Group</field>
        <field name="category_id" ref="module_category_fleet"/>
    </record>

    <record id="group_fleet_emp" model="res.groups">
        <field name="name">Employee</field>
        <field name="comment">Employee Group</field>
        <field name="category_id" ref="module_category_fleet"/>
    </record>

</odoo>